<?xml version="1.0" encoding="UTF-8"?>

<!-- 
    Note:  http://ld4l.org/ontology/cartotek-o/ is a temporary namespace for the LD4P/L Cartographic Materials ontology extension 
  -->

<rdf:RDF
    xmlns="http://ld4l.org/ontology/cartotek-o/"
    xmlns:bib="http://bibliotek-o.org/ontology/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:foaf="http://xmlns.com/foaf/0.1#"
    xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
    >
    
    <!-- ONTOLOGY DECLARATION -->
    
    <owl:Ontology rdf:about="http://ld4l.org/ontology/cartotek-o">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version 0.0</owl:versionInfo>
        <rdfs:label xml:lang="en">cartotek-o: extension ontology for describing cartographic materials</rdfs:label>
        <vann:preferredNamespacePrefix>cart</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">cartotek-o is an extension ontology of the bibliotek-o BIBFRAME 2.0 extension ontology produced by the Linked Data for Libraries Cartographic Materials project for use in describing cartographic resources</rdfs:comment>
    </owl:Ontology>
    
    
    <!-- CLASSES -->
    
    <!-- Class http://id.loc.gov/ontologies/bibframe/Identifier -->
    
    <owl:Class rdf:about="http://ld4l.org/ontology/cartotek-o/EpsgCode">
        <rdfs:label>EPSG Code</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <skos:definition xml:lang="en-us">EPSG Dataset Code</skos:definition>
    </owl:Class>
    
    <!-- OBJECT PROPERTIES -->
    
    
    
    <!-- NAMED INDIVIDUALS -->
    
    <!-- Type http://id.loc.gov/ontologies/bibframe/Projection 
    Adding subtypes MARC coded projection types: https://www.loc.gov/marc/bibliographic/bd008p.html -->
    
    <owl:NamedIndividual rdf:about="http://ld4l.org/ontology/cartotek-o/Mercator">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Projection"/>
        <bf:identifiedBy rdf:resource="http://ld4l.org/ontology/cartotek-o/EpsgCode4326"/>
        <rdfs:label xml:lang="en">Mercator</rdfs:label>
        <skos:definition xml:lang="en">a projection of a map of the world onto a cylinder in such a way that all the parallels of latitude have the same length as the equator, used especially for marine charts and certain climatological maps.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://ld4l.org/ontology/cartotek-o/Test-Projection">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Projection"/>
        <rdfs:label xml:lang="en">Test Projection</rdfs:label>
        <skos:definition xml:lang="en">a projection of a map of the world onto a cylinder in such a way that all the parallels of latitude have the same length as the equator, used especially for marine charts and certain climatological maps.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:NamedIndividual>

    
    <!-- Type http://ld4l.org/ontology/cartotek-o/EpsgCode
    Individuals of type  EpsgCode -->
    
    <owl:NamedIndividual rdf:about="http://ld4l.org/ontology/cartotek-o/EpsgCode4326">
        <rdf:type rdf:resource="http://ld4l.org/ontology/cartotek-o/EpsgCode"/>
        <rdfs:label xml:lang="en">WGS 84</rdfs:label>
        <bf:identifies rdf:resource="http://ld4l.org/ontology/cartotek-o/Mercator"/>
        <skos:definition xml:lang="en">World Geodetic System 1984</skos:definition>
        <foaf:page>http://epsg.io/4326</foaf:page>
        <rdf:value>4326</rdf:value>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:NamedIndividual>
    
    
    <!-- Type http://bib.ld4l.org/ontology/hasSource 
    Adding subtypes of MARC coded cartographic data sources: https://www.loc.gov/standards/sourcelist/cartographic-data.html  -->
    
    <owl:NamedIndividual rdf:about="http://ld4l.org/ontology/cartotek-o/Bound">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Source"/>
        <rdfs:label xml:lang="en">Bounding Box (Klokan Technologies)</rdfs:label>
        <skos:definition xml:lang="en">The Klokan Technologies Bounding Box Tool.</skos:definition>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
    </owl:NamedIndividual>
    
</rdf:RDF>

