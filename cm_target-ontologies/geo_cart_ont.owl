<rdf:RDF
    xmlns="http://ontology.library.harvard.edu/geo/"
    xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
    xmlns:bib="http://bibliotek-o.org/ontology/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:foaf="http://xmlns.com/foaf/0.1#"
    >
    
    <!-- ONTOLOGY DECLARATION -->
    
    <owl:Ontology rdf:about="http://ontology.library.harvard.edu/geo/">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version 0.0</owl:versionInfo>
        <rdfs:label xml:lang="en">Geospatial and Cartographic Resources Ontology: extension ontology for describing cartographic materials</rdfs:label>
        <vann:preferredNamespacePrefix>gcro</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">Geospatial and Cartographic Resources Ontology is an extension ontology of the bibliotek-o BIBFRAME 2.0 extension ontology produced by the Linked Data for Libraries Cartographic Materials project for use in describing cartographic resources</rdfs:comment>
    </owl:Ontology>
    
 
    <!-- CLASSES -->
    
    <!-- Class http://id.loc.gov/ontologies/bibframe/Identifier -->
    
    <owl:Class rdf:about="http://ontology.library.harvard.edu/geo/EpsgCode">
        <rdfs:label xml:lang="en">EPSG Code</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <skos:definition xml:lang="en">EPSG Dataset Code</skos:definition>
    </owl:Class>
	
	<!-- Class http://id.loc.gov/ontologies/bibframe/Cartography  
    Adding subclasses of geospatial and cartographic data types -->
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Atlases">
	    <rdfs:label xml:lang="en">Atlases</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A collection of maps designed to be kept (bound or loose) in a volume</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/CartographicModels">
	    <rdfs:label xml:lang="en">Cartographic Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A general category which denotes any three-dimensional representation of an object or geographic area, modeled in any size or medium [not including globes]</skos:definition>
    </owl:Class>
		
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/AstronomicalModels">
	    <rdfs:label xml:lang="en">Astronomical Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicModels"/>
        <skos:definition xml:lang="en">Astronomical models</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ReliefModels">
	    <rdfs:label xml:lang="en">Relief Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicModels"/>
        <skos:definition xml:lang="en">A map whose surface is shaped to represent the topography in a region. Also called a terrain model</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/CartographicProfiles">
	    <rdfs:label xml:lang="en">Cartographic Profile</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/Cartography"/>
        <skos:definition xml:lang="en">A scale representation of the intersection of a vertical surface (which mayh or may not be a plane) with the surface of the ground, or of the intersection of such a vertical surface with that of a conceptual three-dimensional model representing phenomena having a continuous distribution (e.g. rainfall).  The vertical scale is often exaggerated. (AACR2 mod.)</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/CartographicSections">
	    <rdfs:label xml:lang="en">Cartographic Section</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A scale representation of a vertical surface (commonly a plane) displaying both the profile where it intersects the surface of the ground, or some conceptual model, and the underlying structures along the plane of intersection (e.g. a geologic section) (AACR2 mod.)</skos:definition>
    </owl:Class>
		
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/GeologicalCrossSections">
	    <rdfs:label xml:lang="en">Geological Cross-section</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicSections"/>
        <skos:definition xml:lang="en">A diagram or drawing that shows features transected by a given plane; specif. a vertical section drawn at right angles to the longer axis of a geologic feature, such as as the trend of an orebody, the mean direction of flow of a stream, or the axis of a fossil</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/CartographicViews">
	    <rdfs:label xml:lang="en">Cartographic View</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A perspective representation of cities or landscapes</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/AerialViews">
	    <rdfs:label xml:lang="en">Aerial View</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicViews"/>
        <skos:definition xml:lang="en">Representations of cities or landscapes portrayed as if viewed from above</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Panorama">
	    <rdfs:label xml:lang="en">Panorama</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicViews"/>
        <skos:definition xml:lang="en">A perspective representation of the landscape in which the detail is shown as if projected onto a vertical plane or onto the insides of a cylinder centered vertically on the observer. (Ency. Dict. of Cart.)</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/WormsEyeViews">
	    <rdfs:label xml:lang="en">Worm's Eye View</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/CartographicViews"/>
        <skos:definition xml:lang="en">Representations of cities or landscapes portrayed as if viewed from a very low viewpoint.</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/DigitalCartographicResources">
	    <rdfs:label xml:lang="en">Digital Cartographic Resources</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">Digital cartographic resource</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScannedCartographicResources">
	    <rdfs:label xml:lang="en">Scanned Cartographic Resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/DigitalCartographicResources"/>
        <skos:definition xml:lang="en">An analog resources that has been scanned for digital viewing as a static/non-interactive image</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/StaticDigitalCartographicResources">
	    <rdfs:label xml:lang="en">Static Digital Cartographic Resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/DigitalCartographicResources"/>
        <skos:definition xml:lang="en">Born-digital cartographic resource that is static</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/InteractiveDigitalCartographicResources">
	    <rdfs:label xml:lang="en">Interactive Digital Cartographic Resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/DigitalCartographicResources"/>
        <skos:definition xml:lang="en">Born-digital cartographic resource that is interactive</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Ephemerides">
	    <rdfs:label xml:lang="en">Ephemerides</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">Ephemeris plural ephemerides: a tabular statement of the assigned places of a celestial body for regular intervals. Table of the positions of celestial bodies at regular intervals, often with supplementary information. Constructed as early as the 4th century BC, ephemerides are still essential to astronomers and navigators. Modern ephemerides are calculated, with heavy computing and careful checking, after a mathematical description of a heavenly body's observed motion has been evolved. Various national ephemerides are published regularly; the U.S. ephemeris, first published in 1852, became the best and is now published jointly with the U.K. as The Astronomical Almanac.</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Gazetteers">
	    <rdfs:label xml:lang="en">Gazetteer</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A gazetteer is a geographical dictionary or directory, an important reference for information about places and place names, used in conjunction with a map or a full atlas. It typically contains information concerning the geographical makeup of a country, region, or continent as well as the social statistics and physical features, such as mountains, waterways, or roads.</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/GeospatialDigitalData">
	    <rdfs:label xml:lang="en">Geospatial Digital Data</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">Digital geospatial data</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/VectorData">
	    <rdfs:label xml:lang="en">Vector Data</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/GeospatialDigitalData"/>
        <skos:definition xml:lang="en">A data structure used to represent linear geographic features. Features are made of ordered lists of x, y or x, y, z coordinates and are represented by points, lines, or polygons; points connect to become lines, and lines connect to become polygons. Attributes are associated with each feature.</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/RasterData">
	    <rdfs:label xml:lang="en">Raster Data</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/GeospatialDigitalData"/>
        <skos:definition xml:lang="en">Data that are based on a cellular grid structure composed of rows and columns (a raster). Each grid cell or pixel is referenced by an x, y coordinate and is stored as an identifier to which additional attributes may be attached. Groups of cells with the same value represent map features.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/GeoreferencedCartographicResources">
	    <rdfs:label xml:lang="en">Georeferenced Cartographic Resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/GeospatialDigitalData"/>
        <skos:definition xml:lang="en">A scanned, paper map or cartographic resource that has been fit to real world coordinates using a GIS and is enabled for spatial display in a digital environment</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/GeospatialDatabases">
	    <rdfs:label xml:lang="en">Geospatial Database</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/GeospatialDigitalData"/>
        <skos:definition xml:lang="en">A database with geospatial data</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Globes">
	    <rdfs:label xml:lang="en">Globe</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A spherical representation of the earth, a celestial body, or the heavens</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/CelestialGlobes">
	    <rdfs:label xml:lang="en">Celestial Globe</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/Globes"/>
        <skos:definition xml:lang="en">A spherical representation of a celestial body, or the heavens</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/TerrestrialGlobes">
	    <rdfs:label xml:lang="en">Terrestrial Globe</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/Globes"/>
        <skos:definition xml:lang="en">A spherical representation of the earth</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Maps">
	    <rdfs:label xml:lang="en">Map</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">A graphic representation, usually on a plane surface and at an established scale, of natural and manmade features on or under the surface of the earth or other planetary body. 
            The features are positioned as accurately as possible, usually relative to a coordinate reference system. Also, a graphic representation of a part or the whole of the celestial sphere) physical map</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/RemoteSensingImages">
	    <rdfs:label xml:lang="en">Remote Sensing Image</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Cartography"/>
        <skos:definition xml:lang="en">Images of planetary surfaces created by means of reflected or emitted electromagnetic energy</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/AerialPhotographs">
	    <rdfs:label xml:lang="en">Aerial Photograph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/RemoteSensingImages"/>
        <skos:definition xml:lang="en">Images of planetary surfaces created by means of reflected or emitted electromagnetic energy</skos:definition>
    </owl:Class>
	
		<!-- Class http://id.loc.gov/ontologies/bibframe/Scale 
    Adding subclasses of cartographic scales -->
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/HorizontalScale">
	    <rdfs:label xml:lang="en">Horizontal Scale</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Horizontal scale</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/VerticalScale">
	    <rdfs:label xml:lang="en">Vertical Scale</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Vertical scale</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/NotToScale">
	    <rdfs:label xml:lang="en">Not to Scale</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Not to scale</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScaleIndeterminable">
	    <rdfs:label xml:lang="en">Scale Indeterminable</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Scale indeterminable</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScaleNotDetermined">
	    <rdfs:label xml:lang="en">Scale Not Determined</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Scale not determined</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScalesDiffer">
	    <rdfs:label xml:lang="en">Scales Differ</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Scales differ</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScaleVaries">
	    <rdfs:label xml:lang="en">Scale Varies</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Scale"/>
        <skos:definition xml:lang="en">Scale varies</skos:definition>
    </owl:Class>
	
	<!-- Class http://id.loc.gov/ontologies/bibframe/Scale 
    Adding classes related to cartographic scales -->
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScaleAccuracy">
	    <rdfs:label xml:lang="en">Scale Accuracy</rdfs:label>
        <skos:definition xml:lang="en">Scale accuracy</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/ScaleSource">
	    <rdfs:label xml:lang="en">Scale Source</rdfs:label>
        <skos:definition xml:lang="en">Source of the map scale</skos:definition>
    </owl:Class>
	
	<!-- Class http://ontology.library.harvard.edu/geo/Relief 
    Adding Relief class and its subclasses (subclasses based primarily on MARC Map format relief codes (008/18-21) / (006/01-04) -->
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Relief">
        <rdfs:label xml:lang="en">Relief</rdfs:label>
        <skos:definition xml:lang="en">Inequalities of elevation and the configuration of land features on the surface of the Earth that may be represented on maps or charts by contours, hypsometric tints, shading, spot elevations, or hachures. Also encompasses the depiction or indication of underwater topography, or the depths and shapes of underwater terrain</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/LandRelief">
        <rdfs:label xml:lang="en">Land Relief</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/Relief"/>
        <skos:definition xml:lang="en">Inequalities of elevation and the configuration of land features on the surface of the Earth that may be represented on maps or charts by contours, hypsometric tints, shading, spot elevations, or hachures</skos:definition>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Contours">
        <rdfs:label xml:lang="en">Contours</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by contours</skos:definition>
		<rdfs:comment>MARC Map format relief code: a</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Shading">
        <rdfs:label xml:lang="en">Shading</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by shading (generally of a single color)</skos:definition>
		<rdfs:comment>MARC Map format relief code: b</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/GradientTints">
        <rdfs:label xml:lang="en">Gradient Tints</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by gradient tints</skos:definition>
		<rdfs:comment>MARC Map format relief code: c</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Hachures">
        <rdfs:label xml:lang="en">Hachures</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by hachures (short lines which follow the direction of maximum slope)</skos:definition>
		<rdfs:comment>MARC Map format relief code: d</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/FormLines">
        <rdfs:label xml:lang="en">Form Lines</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Land relief shown by form lines</skos:definition>
		<rdfs:comment>MARC Map format relief code: f</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/SpotHeights">
        <rdfs:label xml:lang="en">Spot Heights</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by spot heights</skos:definition>
		<rdfs:comment>MARC Map format relief code: g</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/Pictorially">
        <rdfs:label xml:lang="en">Pictorially</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Land forms and other topographic features shown in the correct planimetric position by pictorial symbols representing their appearance from a high oblique view</skos:definition>
		<rdfs:comment>MARC Map format relief code: i</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/LandForms">
        <rdfs:label xml:lang="en">Land Forms</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief shown by land forms</skos:definition>
		<rdfs:comment>MARC Map format relief code: j</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/RockDrawings">
        <rdfs:label xml:lang="en">Rock Drawings</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/LandRelief"/>
        <skos:definition xml:lang="en">Relief rock drawing</skos:definition>
		<rdfs:comment>MARC Map format relief code: m</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/WaterRelief">
        <rdfs:label xml:lang="en">Water Relief</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/Relief"/>
        <skos:definition xml:lang="en">The depiction or indication of underwater topography, or the depths and shapes of underwater terrain</skos:definition>
		<rdfs:comment>MARC Map format relief code: </rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/BathymetricTints">
        <rdfs:label xml:lang="en">Bathymetric Tints</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/WaterRelief"/>
        <skos:definition xml:lang="en">Underwater relief shown by bathymetric tints</skos:definition>
		<rdfs:comment>MARC Map format relief code: c</rdfs:comment>
    </owl:Class>
	
	<owl:Class rdf:about="http://ontology.library.harvard.edu/geo/BathymetricIsolines">
        <rdfs:label xml:lang="en">Bathymetric Isolines</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ontology.library.harvard.edu/geo/WaterRelief"/>
        <skos:definition xml:lang="en">Underwater relief (depth) shown by isolines (lines that represent constant depths)</skos:definition>
